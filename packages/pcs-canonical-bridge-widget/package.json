{
  "name": "@bnb-chain/pcs-canonical-bridge-widget",
  "version": "0.0.1",
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "coverage": "vitest run --coverage",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "typechecks": "tsc --noEmit -p ./tsconfig.json"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "dependencies": {
    "@pancakeswap/localization": "workspace:*",
    "@bnb-chain/canonical-bridge-widget": "0.5.2",
    "@tanstack/react-query": "^5.52.1",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.1",
    "axios": "~1.6.8",
    "polished": "~4.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "6.0.7"
  },
  "devDependencies": {
    "@pancakeswap/tsconfig": "workspace:*",
    "@pancakeswap/utils": "workspace:*",
    "@pancakeswap/uikit": "workspace:*",
    "@vanilla-extract/vite-plugin": "^3.8.0",
    "@types/lodash": "^4.14.168",
    "@vitejs/plugin-react": "4.2.1",
    "tsup": "^6.7.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  }
}
